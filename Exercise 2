SQL  EXERCISE 2  

QUESTION 1

select * from S order by city desc;

-------------------------------------------------------------------------------------------------------------------------------------------------------------------------
QUESTION 2

 select * from P order by city, Pname;

-------------------------------------------------------------------------------------------------------------------------------------------------------------------------
QUESTION 3

select * from S where Status1 between 10 and 20;

-------------------------------------------------------------------------------------------------------------------------------------------------------------------------
QUESTION 4

select * from P where weight not between 10 and 15;

-------------------------------------------------------------------------------------------------------------------------------------------------------------------------
QUESTION 5

select Pname from P where Pname >= 'S%' and pname < 'T%';

-------------------------------------------------------------------------------------------------------------------------------------------------------------------------
QUESTION 6

select * from S where city >= 'L%' and city < 'M%';

-------------------------------------------------------------------------------------------------------------------------------------------------------------------------
QUESTION 7

select * from J where Jname like '__n%';

